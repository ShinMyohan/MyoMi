<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- SQL definition -->
<mapper namespace="MembershipMapper" > 

<resultMap type="Membership" id="Membership" autoMapping="true">
<id property="membership_num" column="membershipId" />
	<result column="membership_level" property="membershipLevel" />

</resultMap>
	<select id="selectAll" resultType="Membership">
	SELECT
		membership_num,
		membership_level
		FROM membership
		
	</select>
</mapper>